package ngay2804;

import java.util.HashMap;
import java.util.Scanner;

public class slide85 {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		HashMap<String,String> newHs= new HashMap<>();
		System.out.println("Nhap vao so luong can them");
		int n =sc.nextInt();
		for(int i=0;i<n;i++) {
			System.out.println("Nhap gia tri key=");
			String key=sc.nextLine();
			System.out.println("Nhap gia tri value=");
			String value =sc.nextLine();sc.nextLine();
			newHs.put(key, value);
		}
		System.out.println(newHs);
	
}
}
